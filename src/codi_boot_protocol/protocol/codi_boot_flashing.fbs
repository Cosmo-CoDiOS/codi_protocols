namespace CodiBoot.Flashing;

// Enum defines the two (currently) different variants of CoDi.
enum CoDiVariant: byte {
    STOCK = 0, // Stock CoDi enum value is an integer.
    CODIOS = 1, // CoDiOS enum value is an integer.
}

table QueryFlashCommand {}
table QueryFlashResponse {
    fw_version: string;
    fw_variant: CoDiVariant;
}

